group DIALOG ;

mypfad() ::= "frontend"


dateiName(urmodell,model) ::= "$mypfad()$/$model.Page$.html"

dateiInhalt(urmodell,model) ::= <<

$element(model)$

>>

element(x) ::= <<
 
  <$x.LocalName$  $attributes(x)$ >
	$x.Childs:element()$
  </$x.LocalName$>
 
 >>

attributes(x) ::= << $x.Attributes:attribute()$ >>

attribute(x) ::= << $x.LocalName$ = "$x.Value$" >>
