group DIALOG ;

overwrite(urmodell,model) ::= "true"

mypfad() ::= "package"

dateiName(urmodell,model) ::= "$mypfad()$/$model.SimpleName$.java"

dateiInhalt(urmodell,model) ::= <<

$DATA(model,model.SimpleName,mypfad())$

>>

DATA(x,classname,pfad) ::= <<
package $pfad$.pages;


import java.io.Serializable;
import swingapp.data.*;
import janus.tech.model.*;


public class $classname$ extends DataDIALOG<Global> {

}

>>



